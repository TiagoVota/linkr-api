{
	"env": {
			"es2021": true,
			"node": true,
			"jest": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
	},
	"rules": {
			"indent": [
					"error",
					"tab"
			],
			"linebreak-style": [
					"error",
					"unix"
			],
			"quotes": [
					"error",
					"single"
			],
			"semi": [
					"error",
					"never"
			],
			"no-unused-vars": "off"
	}
}
